{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev --port 3000",
    "dev": "next dev --port 3000",
    "build": "next build",
    "translations": "formatjs extract './{components,pages}/**/*.{ts,tsx}' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file locale/en-US.json && rm ./temp.json",
    "analyze-build": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "lint:staged": "next lint --dir . --fix --quiet",
    "check-types": "tsc --noEmit",
    "generate": "dotenv -c -- graphql-codegen --config codegen.yml",
    "paths": "pathpida --ignorePath ../../.gitignore",
    "clean": "rm -rf .next .turbo"
  },
  "peerDependencies": {
    "@urql/core": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "3.10.3",
    "@graphql-codegen/typescript-apollo-client-helpers": "3.0.0",
    "@headlessui/react": "2.0.3",
    "@heroicons/react": "2.1.3",
    "@saleor/auth-sdk": "1.0.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.13",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.isequal": "^4.5.8",
    "@strapi/blocks-react-renderer": "1.0.1",
    "algoliasearch": "4.19.1",
    "clsx": "2.1.1",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.2",
    "editorjs-html": "3.4.3",
    "graphql": "16.8.1",
    "graphql-tag": "^2.12.6",
    "js-cookie": "3.0.5",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "4.5.0",
    "next": "14.2.3",
    "next-nprogress-bar": "2.3.12",
    "next-usequerystate": "1.17.2",
    "plaiceholder": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.4",
    "react-instantsearch": "7.9.0",
    "react-intl": "6.6.6",
    "react-zoom-pan-pinch": "^3.4.4",
    "sharp": "^0.33.3",
    "swiper": "^11.1.1",
    "tailwind-scrollbar-hide": "1.1.7",
    "ts-invariant": "0.10.3",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.2.10",
    "@graphql-codegen/add": "5.0.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-react-apollo": "4.3.0",
    "@next/bundle-analyzer": "14.2.3",
    "@next/env": "14.0.4",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.12.11",
    "@types/qs": "6.9.15",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.3.0",
    "@types/url-join": "4.0.3",
    "autoprefixer": "10.4.19",
    "eslint": "9.2.0",
    "formik": "^2.4.5",
    "nookies": "^2.5.2",
    "pathpida": "0.22.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "react-use": "17.4.0",
    "schema-dts": "1.1.2",
    "tailwindcss": "3.4.3",
    "typescript": "5.1.6",
    "xss": "1.0.14",
    "yup": "1.4.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "14 || ^16 || ^18 || ^20 || ^21",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.1.4"
}
